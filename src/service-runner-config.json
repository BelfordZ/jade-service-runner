{
  "$schema": "./lib/service-runner-schema.json",
  "services": [{
    "name": "multi-geth",
    "rpcPort":"${DYNAMIC_TCP_PORT_1}",
    "version": "1.8.5",
    "environments": [{
        "name": "mainnet",
        "args": {
          "start": ["--datadir", "${SERVICE_DIR}", "/multi-geth/1.8.5"],
          "stop": [],
          "teardown":[]
        }
      },
      {
        "name": "gorli",
        "args": {
          "start": [],
          "stop": [],
          "teardown":[] 
        }
      },
      {
        "name": "rinkeby",
        "args": {
          "start": [],
          "stop": [],
          "teardown": [] 
        }
      }
    ],
    "os": {
      "osx": {
        "commands": {
          "setup": [
            {
              "cmd": "chmod",
              "args": ["+x", "geth"]
            }
          ],
          "start": "./geth",
          "stop": "",
          "teardown": ""
        },
        "assets": ["https://github.com/multi-geth/multi-geth/releases/download/v1.8.27/multi-geth-osx.zip"]
      },
      "linux": {
        "commands": {
          "setup": [],
          "start": "geth",
          "stop": "",
          "teardown": ""
        },
        "assets": ["https://github.com/multi-geth/multi-geth/releases/download/v1.8.27/multi-geth-linux.zip"]
      }
    }
  }]
}