{
  "name": "jade-service-runner",
  "version": "1.0.0",
  "description": "![](https://www.etclabs.org/dist/resources/images/v2/logo-top.png) Supported by [ETC Labs](https://www.etclabs.org/)",
  "main": "index.js",
  "dependencies": {
    "@open-rpc/client-js": "^1.0.1",
    "@open-rpc/server-js": "^1.1.2",
    "@open-rpc/typings": "^1.3.0",
    "commander": "^2.20.0",
    "follow-redirects": "^1.7.0",
    "fs-extra": "^8.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "tar-fs": "^2.0.0",
    "typedoc": "^0.14.2",
    "winston": "^3.2.1",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@open-rpc/generator-client": "^1.9.2",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@types/body-parser": "^1.17.0",
    "@types/connect": "^3.4.32",
    "@types/cors": "^2.8.4",
    "@types/fs-extra": "^5.1.0",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.132",
    "@types/request": "^2.48.1",
    "@types/rimraf": "^2.0.2",
    "@types/tar-fs": "^1.16.1",
    "@types/yauzl": "^2.9.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "test": "npx jest",
    "generator-client": "open-rpc-generator-client",
    "build": "npm run build:typings && npm run build:client && npm run build:code && npm run build:docs",
    "build:code": "npx tsc",
    "build:typings": "npx open-rpc-typings --output-ts src/generated-types",
    "build:client": "npm run generator-client -- --ts-name=@etclabscore/jade-service-runner --rs-name=jade-service-runner -o src/generated-client",
    "build:docs": "typedoc",
    "publish": "npm run publish:typescript",
    "publish:typescript": "cd src/generated-client/typescript && npm publish --access=public",
    "start": "npm run build && node build/src/cli/index.js",
    "start:client": "node build/client.js",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "test:unit": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etclabscore/jade-service-runner.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/etclabscore/jade-service-runner/issues"
  },
  "files": [
    "build",
    "!build/*.test.*"
  ],
  "homepage": "https://github.com/etclabscore/jade-service-runner#readme"
}
